<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="MauiApp.Renderers.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:pages="clr-namespace:MauiApp.Renderers.Pages"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:MauiApp.Renderers"
    Shell.FlyoutBehavior="Disabled"
    Shell.NavBarIsVisible="False">

    <TabBar>
        <Tab>
            <ShellContent ContentTemplate="{DataTemplate pages:OnePage}" />
        </Tab>
        <Tab>
            <ShellContent ContentTemplate="{DataTemplate pages:TwoPage}" />
        </Tab>
        <Tab>
            <ShellContent ContentTemplate="{DataTemplate pages:ThreePage}" />
        </Tab>
    </TabBar>
</Shell>
